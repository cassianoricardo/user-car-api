#!/usr/bin/env groovy
pipeline {
    agent any
    tools {
        maven 'Maven 3.9.4'
        jdk 'jdk18'
    }
    stages {
        stage('Deployment') {
            steps {
             bat "FOR /F \"tokens=5 delims= \" %%G IN (\"netstat -a | findstr :8081\") DO TaskKill.exe /PID %%G /fi \"memusage gt 0\""
             bat 'start mvnw.cmd spring-boot:run -Dserver.port=8989'
            }
        }
    }
}